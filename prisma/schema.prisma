// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:tmrisn.db"
}

model SerialNumber{
  id Int @id @default(autoincrement())
  snType  String?
  city  String
  sn  String
  editTime  DateTime @default(now())
  editUser  String
}

model User{
  id Int @id @default(autoincrement())
  userName String
  password String
  email String?
  phone String?
  realName  String
  role  String
}